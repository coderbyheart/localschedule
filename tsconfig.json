{
  "extends": "@bifravst/code-style/tsconfig.json",
  "include": ["src/*.tsx", "src/*.ts"],
  "compilerOptions": {
    "outDir": "dist/",
    "esModuleInterop": true,
    "module": "es6",
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "allowJs": true
  }
}
